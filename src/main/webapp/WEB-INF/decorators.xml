<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/supervisor/login</pattern>
		<pattern>/supervisor/login?error=true</pattern>
	</excludes>
	
	<decorator name="frontMain" page="frontMain.jsp">
		<pattern>/*</pattern>
		<pattern>/front/*</pattern>
	</decorator>
 	<decorator name="supervisorMain" page="supervisorMain.jsp">
		<pattern>/supervisor/*</pattern>
	</decorator>
	
</decorators>
